<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>
		<!-- report theme -->

		<!-- standard, dark -->

		<theme>standard</theme>
		<!-- document encoding -->

		<!-- defaults to UTF-8 -->

		<encoding>UTF-8</encoding>
		<!-- protocol for script and stylesheets -->

		<!-- defaults to https -->

		<protocol>http</protocol>
		<!-- title of the document -->
		<documentTitle>Extent</documentTitle>
		<!-- report name - displayed at top-nav -->
		
		<reportName>
		<![CDATA[
		<img src='src/test/resources/zeiss-logo.png' />
		]]>
        Subscription Automation Execution Report
		</reportName>
		<!-- location of charts in the test view -->
		<!-- top, bottom -->

		<testViewChartLocation>bottom</testViewChartLocation>
		<!-- custom javascript -->

		<scripts>
		<![CDATA[
		$(document).ready(function() {
		$(".brand-logo").hide();
		});
		]]>
		</scripts>
		<!-- custom styles -->
		<styles>
		<![CDATA[
		.report-name{padding-left:10px;} .report-name > img { float:left;height: 100%;margin-left: 10px;margin-top: 1px;width: auto; }
		]]>
		</styles>
	</configuration>
</extentreports>